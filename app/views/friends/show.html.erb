
<div class="jumbotron">
  <h1 class="display-4">
  <%= @friend.first_name %>
  <%= @friend.last_name %>
  </h1>
  <p class="lead">
  <%= @friend.email %> |
  Tel:
  <%= @friend.phone %> |
  Twitter:
  <%= @friend.twitter %>
  </p>
  <hr class="my-4">
  <%= link_to 'Edit', edit_friend_path(@friend), class:'btn btn-primary' %> 
  <%= link_to 'Back', friends_path, class:'btn btn-primary' %> 
</div>

